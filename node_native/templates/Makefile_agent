# -*-Makefile-*-

FOS_DIR = /etc/fos
FOS_CONF_FILE = $(FOS_DIR)/agent.json


all:

	dune build

clean:
	dune clean


test:
	echo "Nothing to do"

install:
	mkdir -p $(FOS_DIR)
	install -m 0755 _build/default/fos-agent/fos_agent.exe $(FOS_DIR)/agent
	install -m 0755 to_uuid.sh $(FOS_DIR)/to_uuid.sh
    install etc/agent.json  $(FOS_DIR)/agent.json
